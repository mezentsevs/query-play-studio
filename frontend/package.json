{
  "name": "query-play-studio-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint src/ --ext .js,.ts",
    "lint:fix": "eslint src/ --ext .js,.ts --fix",
    "vue:lint": "eslint src/ --ext .vue",
    "vue:lint:fix": "eslint src/ --ext .vue --fix",
    "ts:lint": "eslint src/ --ext .ts",
    "ts:lint:fix": "eslint src/ --ext .ts --fix",
    "format": "prettier --check \"src/**/*.{js,ts,vue}\"",
    "format:fix": "prettier --write \"src/**/*.{js,ts,vue}\"",
    "code:lint": "npm run lint && npm run vue:lint && npm run format",
    "code:fix": "npm run format:fix && npm run lint:fix && npm run vue:lint:fix"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "axios": "^1.6.0",
    "dompurify": "^3.3.1",
    "highlight.js": "^11.9.0",
    "marked": "^17.0.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.7",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@types/dompurify": "^3.0.5",
    "@types/highlight.js": "^10.1.0",
    "@types/marked": "^5.0.2",
    "@types/node": "^24.10.2",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.32.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-vue": "^10.3.0",
    "globals": "^16.3.0",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.8"
  }
}
